<?php
// Question no : 26 - Remove duplicates from sorted array

class Solution {

    /**
     * @param Integer[] $nums
     * @return Integer
     */
    function removeDuplicates(&$nums) {
      $unique = [];
      $first = $nums[0];
      array_push($unique,$first);
      for($i = 0; $i < count($nums); $i++){
          if($nums[$i] != $first){
              $unique[] = $nums[$i];
              $first = $nums[$i];
          }
      }
      $nums = $unique;
      print_r($nums);
    }
}

?>
