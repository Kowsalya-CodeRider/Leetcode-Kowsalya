<?php
// Question 27: Remove Element

class Solution {

    /**
     * @param Integer[] $nums
     * @param Integer $val
     * @return Integer
     */
    function removeElement(&$nums, $val) {
        $output = array();
        for($i = 0; $i < count($nums); $i++){
            if($nums[$i] != $val){
                $output[$i] = $nums[$i];
            }
        }
        $nums = $output;
        print_r($nums);
    }
}

?>
